{
  "name": "shahidparvez",
  "version": "1.0.0",
  "description": "Making This Blog Ready for Github pages Automation",
  "main": "index.js",
  "scripts": {
    "badge": "node helper/badges.js",
    "build": "npm run hugo:build",
    "clean": "npm run hugo:clean",
    "serve": "hugo serve --environment production",
    "hugo:build": "hugo -d build",
    "hugo:serve": "npm run badge && npm run serve",
    "hugo": "hugo --minify --environment production",
    "hugo:clean": "rm -rf build resources public",
    "start": "npm run build && npm run hugo"
  },
